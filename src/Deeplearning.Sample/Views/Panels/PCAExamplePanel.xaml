<UserControl x:Class="Deeplearning.Sample.Views.Panels.PCAExamplePanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Deeplearning.Sample.Views.Panels" xmlns:oxy="http://oxyplot.org/wpf"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition Width="100"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <oxy:PlotView  Grid.Row="0" Grid.Column="0"  Model="{Binding PlotModel}"></oxy:PlotView>
        <StackPanel Grid.Column="1">
            <Button Margin="3" Command="{Binding LoadTestDataCommand}">加载数据</Button>
            <Button  Margin="3" Command="{Binding PCACommand}">主成分分析</Button>
        </StackPanel>
    </Grid>
</UserControl>
